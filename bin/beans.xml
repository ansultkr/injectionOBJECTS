<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
	xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation = "http://www.springframework.org/schema/beans
					http://www.springframework.org/schema/beans/spring-beans.xsd">
					
<bean id="stud" class = "com.ansul.thakur.Student">
	<property name="id" value="1001"/>
	<property name="mathCheat">
		<bean class="com.ansul.thakur.MathCheat"></bean>
	</property>
</bean>

<bean id = "anotherstud" class="com.ansul.thakur.AnotherStudent">
	<property name="cheat">
		<bean class="com.ansul.thakur.MathCheat"/>
	</property>
</bean>

</beans>